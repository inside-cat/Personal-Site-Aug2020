  <section class="contact-sec" id="contact">
    <div class="contact-form">
      <h2>Contact me</h2>
      <form class="form-group" action="/contact" id="contact-form" method="post" role="form">
        <fieldset>

          <input
            class="form-control top"
            id="name"
            name="name"
            type="text"
            placeholder="Your name"
            required>

          <input
            class="form-control middle"
            id="email"
            name="email"
            type="text"
            placeholder="Your email"
            required>

          <textarea class="form-control bottom" id="message" name="message" placeholder="Enter your message here" rows="3" required></textarea>

          <button class="btn btn-lg btn-dark btn-block contact-submit-button" type="submit">Submit</button>
        </fieldset>
      </form>
    </div>

  </section>

